                {
                    "key": "videoId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "type",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 10,
                    "default": null
                }
            ],
            "indexes": [
                {
                    "key": "userId_videoId",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "userId",
                        "videoId"
                    ],
                    "orders": [
                        "ASC",
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "video_interactions",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")"
            ],
            "databaseId": "database",
            "name": "Video Interactions",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "videoId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
