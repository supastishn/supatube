            ],
            "indexes": [
                {
                    "key": "videoId_index",
                    "type": "key",
                    "status": "available",
                    "attributes": [
                        "videoId"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "video_counts",
            "$permissions": [
                "read(\"any\")"
            ],
            "databaseId": "database",
            "name": "Video Counts",
            "enabled": true,
            "documentSecurity": false,
            "attributes": [
                {
                    "key": "likeCount",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 999999999,
                    "default": 0
                },
                {
                    "key": "dislikeCount",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 999999999,
                    "default": 0
                },
                {
                    "key": "commentsJson",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 10000000,
