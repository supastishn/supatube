        {
            "$id": "database",
            "name": "Database",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "comments-interactions",
            "$permissions": [
                "create(\"users\")"
            ],
            "databaseId": "database",
            "name": "Comments Interactions",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "videoId",
                    "type": "string",
                    "required": true,
                    "size": 100,
                    "array": false
                },
                {
                    "key": "commentText",
                    "type": "string",
                    "required": true,
                    "size": 2000,
                    "array": false
                },
                {
                    "key": "parentCommentId",
                    "type": "string",
                    "required": false,
                    "size": 100,
                    "array": false
                },
                {
                    "key": "temporaryClientId",
                    "type": "string",
                    "required": true,
                    "size": 100,
                    "array": false
                },
                {
                    "key": "type",
                    "type": "string",
                    "required": false,
                    "size": 10,
                    "default": "create",
                    "array": false
                },
                {
                    "key": "commentIdToDelete",
                    "type": "string",
                    "required": false,
                    "size": 100,
                    "array": false
                }
            ],
            "indexes": []
        },
        {
            "$id": "video-processing",
            "$permissions": [
                "create(\"users\")"
            ],
            "databaseId": "database",
            "name": "Video Processing Queue",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                { "key": "title", "type": "string", "required": true, "size": 100, "array": false },
                { "key": "description", "type": "string", "required": false, "size": 1000, "array": false },
                { "key": "duration", "type": "integer", "required": true, "min": 0, "array": false },
                { "key": "uncompressedFileId", "type": "string", "required": true, "size": 100, "array": false },
                { "key": "thumbnailId", "type": "string", "required": true, "size": 100, "array": false },
                { "key": "status", "type": "string", "required": false, "size": 15, "default": "pending", "array": false },
                { "key": "errorMessage", "type": "string", "required": false, "size": 500, "array": false }
            ],
            "indexes": [
                {
                    "key": "status_index",
                    "type": "key",
                    "status": "available",
                    "attributes": ["status"],
                    "orders": ["ASC"]
                }
            ]
        },
        {
            "$id": "user_video_states",
            "$permissions": [
                "read(\"users\")"
            ],
            "databaseId": "database",
            "name": "User Video States",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "size": 100,
                    "array": false
                },
                {
                    "key": "videoId",
                    "type": "string",
                    "required": true,
                    "size": 100,
                    "array": false
                },
                {
                    "key": "state",
                    "type": "string",
                    "required": true,
                    "size": 10,
                    "array": false
                }
            ],
            "indexes": [
                {
                    "key": "userId_videoId",
                    "type": "key",
                    "status": "available",
                    "attributes": ["userId", "videoId"],
                    "orders": ["ASC", "ASC"]
                },
                {
                    "key": "userId",
                    "type": "key",
                    "status": "available",
                    "attributes": ["userId"],
                    "orders": ["ASC"]
                }
            ]
        },
        {
            "$id": "videos",
            "$permissions": [
                "read(\"guests\")",
                "create(\"users\")",
                "read(\"users\")"
            ],
            "databaseId": "database",
            "name": "Videos",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "title",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null
                },
                {
                    "key": "description",
                    "type": "string",
                    "required": false,
                    "array": false,
                    "size": 1000,
                    "default": null
                },
                {
                    "key": "video_id",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 500,
                    "default": null
                },
                {
                    "key": "thumbnail_id",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 500,
                    "default": null
                },
                {
                    "key": "video_duration",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "min": 0,
                    "max": 86400,
                    "default": null
                },
                {
                    "key": "likeCount",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 999999999,
                    "default": 0
                },
                {
                    "key": "dislikeCount",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 999999999,
                    "default": 0
                }
            ],
            "indexes": [
                {
                    "key": "title",
                    "type": "fulltext",
                    "status": "available",
                    "attributes": [
                        "title"
                    ],
                    "orders": [
                        "ASC"
                    ]
                }
            ]
        },
        {
            "$id": "accounts",
            "$permissions": [
                "create(\"users\")",
                "read(\"users\")",
                "read(\"guests\")"
            ],
            "databaseId": "database",
